---
import '../styles/cssReset.astro';
import '../styles/indexStyle.astro';
import { Image } from 'astro:assets';
import Header from '../components/header.astro';
import Footer from '../components/footer.astro';
import ContentOne from '../components/contentOne.astro';
import ContentTwo from '../components/contentTwo.astro';
import ContentTitle from '../components/contentTitle.astro';
import ContentPhoto from '../components/contentPhoto.astro';
import ContentMedia from '../components/contentMedia.astro';
import Icons from '../components/icons.astro';
---

<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Jessica Jama</title>
	<!-- <link rel='stylesheet' href='style.css'> -->
	<link
		rel="apple-touch-icon"
		sizes="180x180"
		href="/src/assets/img/apple-touch-icon.png"
	/>
	<link
		rel="icon"
		type="image/png"
		sizes="32x32"
		href="/src/assets/img/favicon-32x32.png"
	/>
	<link
		rel="icon"
		type="image/png"
		sizes="16x16"
		href="/src/assets/img/favicon-16x16.png"
	/>
	<link rel="manifest" href="src/assets/img/site.webmanifest" />
	<!-- <script
		src="https://kit.fontawesome.com/e8137c5d9d.js"
		crossorigin="anonymous"></script>
	<script src="index.js" defer></script> -->
</head>

<Header />
<ContentOne>
	<ContentTitle>
		Senior Product Manager at
		<br />
		<a href="https://www.hand2mind.com/">Hand2Mind</a> in Vernon Hills, Illinois
	</ContentTitle>
	<br />
	<ContentPhoto>
		<Image
			src="/src/assets/img/headshot.png"
			alt="A headshot of Jessica Jama"
			width="341"
			height="512"
		/>
	</ContentPhoto>
</ContentOne>
<ContentTwo>
	<ContentTitle>Links</ContentTitle>
	<Icons />
	<br />
</ContentTwo>
<ContentTitle>Media</ContentTitle>
<br />
<ContentMedia>
	<div class="mediaContainer">
		<div class="mediaBlock">
			<iframe
				class="mediaVid"
				width="560"
				height="315"
				src="https://www.youtube.com/embed/Qvt712E3ynk?si=c57pMIBOmKN1svnL"
				title="YouTube video player"
				frameborder="0"
				allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
				referrerpolicy="strict-origin-when-cross-origin"
				allowfullscreen></iframe>
		</div>
		<div class="mediaText">2023 Toy Fair Panel</div>
	</div>
	<div class="mediaContainer">
		<div class="mediaBlock">
			<button class="mediaBtn" id="mediaBtn2"
				>Social Emtional Learning <br /> During the Holidays
			</button>
		</div>
		<div class="mediaText">The Genius of Play Article</div>
	</div>
	<div class="mediaContainer">
		<div class="mediaBlock">
			<button class="mediaBtn" id="mediaBtn3"
				>Why Self-Care and SEL <br /> Toys Are on the Rise
			</button>
		</div>
		<div class="mediaText">Kidscreen Article</div>
	</div>
	<div class="mediaContainer">
		<div class="mediaBlock">
			<iframe
				class="mediaVid"
				width="560"
				height="315"
				src="https://www.youtube.com/embed/i69taU9hGmw?si=pMAhi0MJ0hrSD3Hi&start=123"
				title="YouTube video player"
				frameborder="0"
				allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
				referrerpolicy="strict-origin-when-cross-origin"
				allowfullscreen></iframe>
		</div>
		<div class="mediaText">2023 Toy Fair News Segment</div>
	</div>
</ContentMedia>

<Footer />
