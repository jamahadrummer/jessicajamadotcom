---
import '../components/style/cssReset.astro';
import '../components/style/index.css';
import Header from '../components/header.astro';
import Footer from '../components/footer.astro';
---

<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Links</title>
	</head>
	<Header />
	<body>
		<div class="content3">
			<div class="content3title">
				Links
				<div class="links">
					<div class="linkBlock">
						<div class="icons">
							<div class="iconContainer">
								<div class="iconSquare">
									<button class="iconBtn" id="iconBtn1"
										><img
											src="/src/assets/img/linkedin.svg"
											alt="Flickr Logo External Link"
										/></button
									>
								</div>
								<div class="iconText">Flickr</div>
							</div>
						</div>
					</div>
					<div class="c3box2"></div>
				</div>
			</div>
		</div>
	</body>
    <Footer />
</html>
